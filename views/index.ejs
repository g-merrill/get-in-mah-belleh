<%- include('./partials/headerhtml') %>
<body>
  <h1><%= viewName %></h1>
  <% if (!user) { %>
    <a href="/auth/google">Login with Google</a>
    <% } else { %>
    <a href="/logout">Log Out</a>
    <p>Hello <%= user.name %>!</p>
    <a href="/new">Add a Food Truck!</a>
<% } %>
  <a href="/users/profile">Go to Profile Page</a>
  <% if (trucks.length) { %>
    <ul>
      <% trucks.forEach(truck => { %>
      <li>
        <form action="/<%= truck.id %>?_method=DELETE" method="POST">
          <input type="submit" value="X">
        </form>
        <p><%= truck.applicant %></p>
        <p><%= truck.fooditems %></p>
        <a href="/<%= truck.id %>">See More...</a>
      </li>
      <% }); %>
    </ul>
  <% } %>
</body>
<%- include('./partials/footerhtml') %>