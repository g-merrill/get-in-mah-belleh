<%- include('./partials/headerhtml') %>
<body>
  <h1><%= viewName %></h1>
  <% if (!user) { %>
    <a href="/auth/google">Login with Google</a>
    <% } else { %>
    <a href="/logout">Log Out</a>
    <p>Hello <%= user.name %>!</p>
<% } %>
  <a href="/profile">Go to Profile Page</a>
  <a href="/new">Add a Food Truck!</a>
  <ul>
    <% if (trucks.length) { %>
      <% trucks.forEach(truck => { %>
      <li><%= truck.applicant %></li>
      <li><%= truck.fooditems %></li>
      <% }); %>
    <% } %>
  </ul>
</body>
<%- include('./partials/footerhtml') %>